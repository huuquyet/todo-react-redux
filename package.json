{
  "name": "todo-react-redux",
  "version": "0.7.0",
  "description": "Todo app with React, Redux, and Firebase",
  "homepage": "https://github.com/r-park/todo-react-redux",
  "repository": {
    "type": "git",
    "url": "https://github.com/r-park/todo-react-redux.git"
  },
  "author": {
    "name": "Richard Park",
    "email": "objectiv@gmail.com"
  },
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=8.1"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production NODE_PATH=. webpack --progress",
    "clean": "del-cli target",
    "lint": "eslint -c .eslintrc.yml --fix *.js src server",
    "prebuild": "run-s clean lint",
    "server": "cross-env NODE_ENV=development nodemon -w 'server/**/*.*' server/main.js",
    "server:dev": "cross-env NODE_ENV=development webpack-dev-server --progress",
    "start": "cross-env NODE_PATH=. npm run server:dev",
    "test": "cross-env --env=jsdom NODE_PATH=. NODE_ENV=test react-scripts test",
    "test:ci": "cross-env CI=true npm test",
    "snyk-protect": "snyk-protect",
    "prepublish": "npm run snyk-protect"
  },
  "dependencies": {
    "express": "^4.18.2",
    "firebase": "9.16.0",
    "html-webpack-plugin": "^5.5.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router": "^3.2.6",
    "react-router-redux": "^4.0.8",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2",
    "reselect": "^4.1.7",
    "webpack": "^5.75.0"
  },
  "devDependencies": {
    "@snyk/protect": "^1.1094.0",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.3.2",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "del-cli": "^5.0.0",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.32.2",
    "immutable": "^4.2.3",
    "mini-css-extract-plugin": "^2.7.2",
    "minx": "^0.4.14",
    "morgan": "^1.10.0",
    "node-sass": "^8.0.0",
    "nodemon": "^2.0.20",
    "react-hot-loader": "^4.13.1",
    "react-scripts": "^5.0.1",
    "sass-loader": "^13.2.0",
    "sinon": "^15.0.1",
    "style-loader": "^3.3.1",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "winston": "^3.8.2"
  },
  "snyk": true,
  "browserLists": [
    "last 3 versions"
  ]
}
