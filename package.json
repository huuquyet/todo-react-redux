{
  "name": "todo-react-redux",
  "version": "0.7.2",
  "description": "Todo app with React, Redux, and Firebase",
  "homepage": "https://github.com/r-park/todo-react-redux",
  "repository": {
    "type": "git",
    "url": "https://github.com/r-park/todo-react-redux.git"
  },
  "author": {
    "name": "Richard Park",
    "email": "objectiv@gmail.com"
  },
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production NODE_PATH=. webpack --progress",
    "clean": "del-cli target",
    "lint": "eslint -c .eslintrc.yml \"{,!(node_modules)/**/}*.+(js|jsx)\"",
    "lint:fix": "eslint -c .eslintrc.yml \"{,!(node_modules)/**/}*.+(js|jsx)\" --fix",
    "prebuild": "run-s clean lint:fix format",
    "server": "cross-env NODE_ENV=development nodemon -w 'server/**/*.*' server/main.js",
    "server:dev": "cross-env NODE_ENV=development webpack-dev-server --progress",
    "start": "cross-env NODE_PATH=. npm run server:dev",
    "test": "cross-env NODE_PATH=. react-scripts test --env=jsdom",
    "test:ci": "cross-env CI=true NODE_PATH=. react-scripts test --env=jsdom",
    "format": "prettier --write \"{,!(node_modules)/**/}*.+(js|jsx|json|yml|scss|md)\"",
    "snyk-protect": "snyk-protect",
    "prepublish": "npm run snyk-protect",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d target"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "core-js": "^3.27.2",
    "firebase": "9.16.0",
    "immutable": "^4.2.3",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router": "^3.2.6",
    "react-router-redux": "^4.0.8",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2",
    "regenerator-runtime": "^0.13.11",
    "reselect": "^4.1.7",
    "webpack": "^5.75.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-function-bind": "^7.18.9",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@snyk/protect": "^1.1096.0",
    "babel-loader": "^8.3.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "del-cli": "^5.0.0",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "express": "^4.18.2",
    "gh-pages": "^5.0.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.2",
    "minx": "^0.4.14",
    "morgan": "^1.10.0",
    "node-sass": "^8.0.0",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "react-hot-loader": "^4.13.1",
    "react-scripts": "^5.0.1",
    "sass-loader": "^13.2.0",
    "sinon": "^15.0.1",
    "style-loader": "^3.3.1",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "winston": "^3.8.2"
  },
  "snyk": true,
  "browserLists": [
    "last 3 versions",
    "> 0.25%, not dead"
  ]
}
