{
  "name": "todo-react-redux",
  "version": "0.6.0",
  "description": "Todo app with React, Redux, and Firebase",
  "homepage": "https://github.com/r-park/todo-react-redux",
  "repository": {
    "type": "git",
    "url": "https://github.com/r-park/todo-react-redux.git"
  },
  "author": {
    "name": "Richard Park",
    "email": "objectiv@gmail.com"
  },
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=6.4"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --display-chunks --progress",
    "clean": "del-cli target",
    "lint": "eslint -c .eslintrc.yml *.js src server",
    "prebuild": "run-s clean lint test",
    "server": "cross-env NODE_ENV=development nodemon -w 'server/**/*.*' server/main.js",
    "server:dev": "cross-env NODE_ENV=development webpack-dev-server --progress",
    "start": "npm run server:dev",
    "test": "cross-env NODE_ENV=test karma start --single-run",
    "test:watch": "cross-env NODE_ENV=test karma start"
  },
  "dependencies": {
    "babel-polyfill": "~6.26.0",
    "classnames": "~2.2.5",
    "immutable": "~3.8.1",
    "react": "~16.14.0",
    "react-dom": "~15.3.2",
    "react-redux": "~4.4.5",
    "react-router": "~2.8.1",
    "react-router-redux": "~4.0.6",
    "redux": "~3.7.2",
    "redux-thunk": "~2.3.0",
    "reselect": "~4.0.0"
  },
  "devDependencies": {
    "autoprefixer": "~6.4.1",
    "babel-core": "~6.26.3",
    "babel-eslint": "~9.0.0",
    "babel-loader": "~6.2.5",
    "babel-preset-es2015": "~6.14.0",
    "babel-preset-react": "~6.11.1",
    "babel-preset-react-optimize": "~1.0.1",
    "babel-preset-stage-0": "~6.24.1",
    "cross-env": "~7.0.2",
    "css-loader": "~0.28.11",
    "del-cli": "~0.2.0",
    "eslint": "~3.19.0",
    "eslint-plugin-react": "~7.21.4",
    "express": "~4.17.1",
    "extract-text-webpack-plugin": "~1.0.1",
    "firebase": "~3.4.0",
    "firebase-tools": "8.12.1",
    "html-webpack-plugin": "~3.2.0",
    "jasmine-core": "~3.6.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-jasmine": "~4.0.1",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-webpack": "~2.0.13",
    "minx": "r-park/minx.git",
    "morgan": "~1.10.0",
    "node-sass": "~4.14.1",
    "nodemon": "~2.0.4",
    "npm-run-all": "~4.1.5",
    "postcss-loader": "~3.0.0",
    "react-addons-test-utils": "~15.3.2",
    "react-hot-loader": "4.13.0",
    "sass-loader": "~4.1.1",
    "sinon": "~9.2.0",
    "style-loader": "~0.23.1",
    "webpack": "~1.15.0",
    "webpack-dev-server": "~1.16.1",
    "webpack-md5-hash": "~0.0.5",
    "winston": "~3.3.3"
  }
}
