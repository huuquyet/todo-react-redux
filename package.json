{
  "name": "todo-react-redux",
  "version": "0.7.2",
  "description": "Todo app with React, Redux, and Firebase",
  "homepage": "https://github.com/r-park/todo-react-redux",
  "repository": {
    "type": "git",
    "url": "https://github.com/r-park/todo-react-redux.git"
  },
  "author": {
    "name": "Richard Park",
    "email": "objectiv@gmail.com"
  },
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=8.9"
  },
  "babel": {
    "presets": [
      "@babel/preset-react",
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-function-bind"
    ],
    "env": {
      "development": {
        "presets": [
          [
            "@babel/preset-react",
            {
              "development": true
            }
          ]
        ]
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "eslint:recommended",
      "react-app",
      "plugin:prettier/recommended"
    ],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "prettier"
    ],
    "ignorePatterns": [
      "target"
    ],
    "globals": {
      "firebase": true,
      "sinon": true
    }
  },
  "prettier": {
    "bracketSameLine": true,
    "bracketSpacing": false,
    "printWidth": 90,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 2,
    "useTabs": false
  },
  "snyk": true,
  "browserLists": [
    "last 3 versions",
    "> 0.25%, not dead"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production NODE_PATH=. webpack --progress",
    "clean": "del-cli target",
    "lint": "eslint \"{,!(node_modules)/**/}*.+(js|jsx)\"",
    "lint:fix": "eslint \"{,!(node_modules)/**/}*.+(js|jsx)\" --fix",
    "prebuild": "run-s clean lint:fix format",
    "server": "cross-env NODE_ENV=development nodemon -w 'server/**/*.*' server/main.js",
    "server:dev": "cross-env NODE_ENV=development webpack-dev-server --progress",
    "start": "cross-env NODE_PATH=. npm run server:dev",
    "test": "cross-env NODE_PATH=. react-scripts test --env=jsdom",
    "test:ci": "cross-env CI=true NODE_PATH=. react-scripts test --env=jsdom",
    "format": "prettier --write \"{,!(node_modules)/**/}*.+(js|jsx|json|yml|md)\"",
    "snyk-protect": "snyk-protect",
    "prepublish": "npm run snyk-protect",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d target"
  },
  "dependencies": {
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.7",
    "@reduxjs/toolkit": "^1.9.2",
    "connected-react-router": "^6.9.3",
    "core-js": "^3.27.2",
    "firebase": "^9.17.1",
    "immutable": "^4.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^4.3.1",
    "redux-thunk": "^2.4.2",
    "regenerator-runtime": "^0.13.11",
    "reselect": "^4.1.7"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-function-bind": "^7.18.9",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@snyk/protect": "^1.1096.0",
    "babel-loader": "^9.1.2",
    "cross-env": "^7.0.3",
    "del-cli": "^5.0.0",
    "dotenv-webpack": "^8.0.1",
    "enzyme": "^3.11.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "express": "^4.18.2",
    "gh-pages": "^5.0.0",
    "history": "^4.10.1",
    "html-webpack-plugin": "^5.5.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "prop-types": "^15.8.1",
    "react-hot-loader": "^4.13.1",
    "react-scripts": "^5.0.1",
    "sinon": "^15.0.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "winston": "^3.8.2"
  }
}
